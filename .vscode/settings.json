{
  "cSpell.words": [
    "binaryen",
    "cond",
    "curlys",
    "Defun",
    "Deno",
    "EBNF",
    "elif",
    "elifs",
    "impls",
    "Inout",
    "memcpy",
    "Multivalue",
    "popcnt",
    "printstr",
    "rotr",
    "Structs",
    "UFCS",
    "WASM"
  ],
  "debug.javascript.terminalOptions": {
    "skipFiles": ["<node_internals>/**"],
    "outFiles": ["${workspaceFolder}/**/*.mjs"]
  },
  "editor.insertSpaces": false,
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()=[{]}\\|;:'\",.<>/?"
}
